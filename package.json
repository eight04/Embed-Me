{
  "name": "embed-me",
  "version": "0.3.0",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "eslint": "^8.52.0",
    "rollup": "^4.1.4",
    "rollup-plugin-cjs-es": "^3.0.0",
    "userscript-meta-cli": "^0.4.2"
  },
  "repository": "eight04/embed-me",
  "license": "MIT",
  "userscript": {
    "name": "Embed Me!",
    "namespace": "eight04.blogspot.com",
    "grant": [
      "GM_addStyle",
      "GM_registerMenuCommand",
      "GM_getValue",
      "GM_setValue",
      "GM_deleteValue",
      "GM_xmlhttpRequest"
    ],
    "require": "https://greasyfork.org/scripts/371339-gm-webextpref/code/GM_webextPref.js?version=705415",
    "connect": [
      "gfycat.com",
      "soundcloud.com",
      "www.youtube.com"
    ],
    "noframes": true
  },
  "author": "eight04 <eight04@gmail.com>",
  "description": "Embed video, images from links.",
  "scripts": {
    "test": "eslint \"**/*.js\" --cache",
    "build": "node build-module-index && rollup -c",
    "preversion": "npm test",
    "version": "npm run build && git add .",
    "postversion": "git push --follow-tags"
  },
  "eslintIgnore": [
    "dist"
  ],
  "dependencies": {
    "fumen-svg": "^0.1.5"
  }
}
