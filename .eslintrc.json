{
	"env": {
		"browser": true,
    "greasemonkey": true
	},
	"rules": {
		"eqeqeq": 0,
		"camelcase": 0,
		"new-parens": 0,
		"no-unused-vars": [2, {"vars": "local"}],
		"new-cap": [2, {"capIsNew": false}],
		"quotes": 0,
		"no-shadow": 0
	},
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended"
  ]
}
